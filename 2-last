(defun last (lst)

    ((lambda(head tail)
           (cond ((null '(tail)) head)
           (t  (last(tail))))     
    )(car lst)(cdr lst))
)
(print (last '(1 2 3 4)))

(defun last (lst)

    (lambda(head tail)
           (if (eq nil tail) print(head)  last'(tail))  
    )(car lst)(cdr lst)
)
(print(last '(1 2 3 4)))
